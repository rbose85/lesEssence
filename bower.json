{
  "name": "lesEssence",
  "private": "true",
  "dependencies": {
    "ionic": "~1.0.1",
    "angularfire": "~1.1.2",
    "ngCordova": "~0.1.17-alpha",
    "bourbon": "~4.2.3",
    "font-awesome": "~4.4.0"
  },
  "devDependencies": {
    "angular-mocks": "~1.4.3"
  },
  "resolutions": {
    "angular": "1.3.13"
  },
  "overrides": {
    "angular": {
      "main": {
        "development": "angular.js",
        "production": "angular.min.js"
      }
    },
    "angular-animate": {
      "main": {
        "development": "angular-animate.js",
        "production": "angular-animate.min.js"
      }
    },
    "angular-sanitize": {
      "main": {
        "development": "angular-sanitize.js",
        "production": "angular-sanitize.min.js"
      }
    },
    "angular-ui-router": {
      "main": {
        "development": "release/angular-ui-router.js",
        "production": "release/angular-ui-router.min.js"
      }
    },
    "angularfire": {
      "main": {
        "development": "dist/angularfire.js",
        "production": "dist/angularfire.min.js"
      }
    },
    "bourbon": {
      "main": {
        "development": "app/**/*.scss",
        "production": "app/**/*.scss"
      }
    },
    "firebase": {
      "main": {
        "development": "firebase-debug.js",
        "production": "firebase.js"
      }
    },
    "font-awesome": {
      "main": [
        "scss/**/*.scss",
        "fonts/*"
      ]
    },
    "ionic": {
      "main": {
        "development": [
          "release/fonts/*",
          "release/js/ionic.js",
          "release/js/ionic-angular.js",
          "scss/**/*.scss"
        ],
        "production": [
          "release/fonts/*",
          "release/js/ionic.min.js",
          "release/js/ionic-angular.min.js",
          "scss/**/*.scss"
        ]
      }
    },
    "ngCordova": {
      "main": {
        "development": "dist/ng-cordova.js",
        "production": "dist/ng-cordova.min.js"
      }
    }
  }
}
